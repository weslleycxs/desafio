{

	"name": "desafio",
	"version": "1.0.0",
	"description": "",
	"main": "desafio.js",
	"author": "Pliffer",
	"license": "SEE IN LICENSE.md",

	"nodemonConfig": {
		"ignore": [
			"modules/*",
			"app/assets/*"
		]
	},

	"kugel": {

		"modules": {

			"core": [
				"_kugel",
				"cl",
				"blitz",
                "jquery",
                "better-express",
                "ecat-vendor-assets",
                "shortcut"
			],

			"startup": [],

			"start": [
                "desafio-frontend"
            ]

		},

		"config": {
			"gzip": true,
			"morgan": true,
			"session": false,
			"socket": false,
			"cors": true,
			"assets": "assets",
			"views": "views",
			"template_engine": "pug",
			"body_parser": true,
			"jwt": false,
			"database": false,
			"modules": true,
			"state": "development",
			"compileViews": "views/public",
			"compiledViewsDest": "assets",
			"viewsOptions": {
				"pretty": true
			}
		}

	},

	"dependencies": {
		"bcryptjs": "^2.4.3",
		"chokidar": "^3.3.1",
		"colors": "^1.4.0",
		"compression": "^1.7.4",
		"crc": "^3.8.0",
		"dotenv": "^8.1.0",
		"email-templates": "^8.0.7",
		"express": "^4.17.1",
		"express-fileupload": "^1.1.6",
		"express-mysql-session": "^2.1.0",
		"express-session": "^1.16.2",
		"express-socket.io-session": "^1.3.5",
		"fs-extra": "^8.1.0",
		"get-folder-size": "^2.0.1",
		"inquirer": "^7.0.0",
		"js-sha512": "^0.8.0",
		"jsontoxml": "^1.0.1",
		"jsonwebtoken": "^8.5.1",
		"morgan": "^1.9.1",
		"node-fetch": "^2.6.1",
		"node-forge": "^0.10.0",
		"node-html-parser": "^1.1.16",
		"node-ipc": "^9.1.4",
		"node-schedule": "^1.3.2",
		"opn": "^6.0.0",
		"pug": "^3.0.2",
		"request": "^2.88.0",
		"sass": "^1.35.2",
		"socket.io": "^2.3.0",
		"validate": "^5.1.0",
		"walkdir": "^0.4.1",
		"xlsx": "^0.17.0"
	}

}
